                    "name": "GetListPersonProfile",
                    "param": "context, in.GetListRequest, []model.SearchParam",
                    "table": "person_profile pp, LEFT JOIN wardes_profile wp, LEFT JOIN wardes_profile_image wpi, LEFT JOIN user u",
                    "return": "[]interface{}, error",
                    "attribute": [
                        "pp.id",
                        "pp.title_id",
                        "pp.title_name",
                        "pp.first_name",
                        "pp.last_name",
                        "wp.id",
                        "wp.username",
                        "wp.personal_name",
                        "wpi.id",
                        "wpi.path_image",
                        "u.id",
                        "u.client_id",
                        "u.user_code"
                    ],
                    "description": "Retrieve a list of person profiles"
                },
                {
                    "name": "CountListPersonProfile",
                    "param": "context, in.GetListRequest, []model.SearchParam",
                    "table": "person_profile",
                    "return": "int, error",
                    "attribute": [
                        "count(id)"
                    ],
                    "description": "Count the total number of person profile records matching the search criteria"
                }
            ],
